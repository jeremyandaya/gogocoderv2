<div class="container">
	<div class="row">
		<div class="col-md-12">
			<div class="jumbotron" id="jumbo-welcome">
				<h1>Hi <%= "#{@coder.first_name}" %>!</h1>
				<p>What do you want to do today?</p>
				<p><%= link_to "Create New Code", new_code_path, class: "btn btn-info btn-lg" %>&nbsp;&nbsp;&nbsp;<a class="btn btn-success btn-lg">Choose Exercise Below</a></p>
			</div>
		</div>
	</div>
	<div class="row" id="welcome-box-row">

		<div class="col-md-2" id="welcome-box">
			<div class="panel panel-primary">
				<div class="panel-heading">
					<h3 class="panel-title">Level 1</h3>
				</div>
				<div class="panel-body">
					<%= link_to "HTML", html_exercise1_path, class: "btn btn-success" %>
				</div>
			</div>
		</div>

		<div class="col-md-2" id="welcome-box">
			<div class="panel panel-success">
				<div class="panel-heading">
					<h3 class="panel-title">Level 2</h3>
				</div>
				<div class="panel-body">
					<%= link_to "CSS", css_exercise1_path, class: "btn btn-primary" %>
				</div>
			</div>
		</div>

		<div class="col-md-2" id="welcome-box">
			<div class="panel panel-warning">
				<div class="panel-heading">
					<h3 class="panel-title">Level 3</h3>
				</div>
				<div class="panel-body">
					<%= link_to "Javascript", javascript_exercise1_path, class: "btn btn-info" %>
				</div>
			</div>
		</div>

		<div class="col-md-2" id="welcome-box">
			<div class="panel panel-danger">
				<div class="panel-heading">
					<h3 class="panel-title">Level 4</h3>
				</div>
				<div class="panel-body">
					<%= link_to "Ruby", html_exercise1_path, class: "btn btn-warning" %>
				</div>
			</div>
		</div>

		<div class="col-md-2" id="welcome-box">
			<div class="panel panel-info">
				<div class="panel-heading">
					<h3 class="panel-title">Level 5</h3>
				</div>
				<div class="panel-body">
					<%= link_to "Rails", html_exercise1_path, class: "btn btn-danger" %>
				</div>
			</div>
		</div>
	</div>
</div>






